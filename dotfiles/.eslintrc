{
  "extends": "eslint:recommended",
  "rules": {
    "semi": [2, "never"],
    "no-shadow": 2,
    "default-case": 2,
    "eqeqeq": 2,
    "no-empty-function": 2,
    "no-empty-pattern": 2,
    "no-extra-bind": 2,
    "no-implicit-coercion": 2,
    "no-implicit-globals": 2,
    "no-invalid-this": 2,
    "no-loop-func": 2,
    "no-magic-numbers": 2,
    "no-redeclare": 2,
    "no-self-assign": 2,
    "no-self-compare": 2,
    "no-unused-expressions": 2,
    "no-undef-init": 2,
    "no-use-before-define": 2,
    "radix": 2,
    "no-duplicate-imports": 2,
    "no-useless-rename": 2,
    "no-var": 2,
    "prefer-const": 2,
    "object-shorthand": 2,
    "prefer-arrow-callback": 2,
    "prefer-rest-params": 2,
    "prefer-spread": 2,
    "prefer-template": 2,
    "react/no-deprecated": 2,
    "react/no-direct-mutation-state": 2,
    "react/no-unknown-property": 2,
    "react/jsx-uses-react": 2,
    "react/jsx-uses-vars": 2,
    "react/jsx-no-duplicate-props": 2,
    "react/jsx-no-undef": 2
  },
  "parserOptions": {
    "ecmaVersion": 6,
    "ecmaFeatures": {
      "experimentalObjectRestSpread": true,
      "jsx": true
    },
    "sourceType": "module"
  },
  "env": {
    "es6": true,
    "node": true,
    "browser": true
  },
  "plugins": [
    "react"
  ]
}
