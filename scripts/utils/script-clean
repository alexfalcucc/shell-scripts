#!/bin/bash
set -euo pipefail

script-log state "Cleaning folders"
find "$HOME"/drive -name 'npm-debug.log' -delete
find "$HOME"/drive -name 'node_modules' -type d -exec rm -r '{}' ';' || true
rm -rf "$HOME"/drive/personal/playground/*
rm -rf "$HOME"/Downloads/*
rm -rf "$HOME"/.Trash
script-log state "Done"
